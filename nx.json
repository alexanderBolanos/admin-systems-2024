{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "nxCloudId": "66f3ae5b5ca8b56efbdaf432",
  "namedInputs": {
    "all": ["{projectRoot}/**/*"],
    "public": ["!{projectRoot}/**/*.test.*"]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["public", "^public"]
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["all", "^public"]
    }
  },
  "options": {
    "cacheableOperations": ["build", "test"]
  }
}
